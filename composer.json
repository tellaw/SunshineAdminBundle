{
    "name": "tellaw/sunshine-admin-bundle",
    "type": "library",
    "description": "Admin Bundle for Symfony",
    "keywords": ["admin", "CRUD", "Back-office", "Dashboard"],
    "homepage": "https://github.com/tellaw/SunshineAdminBundle",
    "license": "MIT",
    "authors": [
      {"name": "Nicolas Robelin"},
      {"name": "Eric Wallet"},
      {"name": "Seth Amegavie"},
      {"name": "Ismail Mezrani"}
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "^3.2.0",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "symfony/monolog-bundle": "^3.0.2",
        "symfony/polyfill-apcu": "^1.0",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "^2.0",
        "twig/twig": "^1.0||^2.0",
        "doctrine/doctrine-fixtures-bundle": "2.3.*",
        "beberlei/DoctrineExtensions" : "1.0.*",
        "tetranz/select2entity-bundle": "2.*",
        "friendsofsymfony/jsrouting-bundle": "^1.6",
        "ninsuo/symfony-collection": "2.1.19",
        "symfony/security-acl": "3.0.*",
        "vich/uploader-bundle": "1.6.*"
    },
    "autoload" : {
      "psr-0" : {
        "Tellaw\\SunshineAdminBundle" : ""
      }
    },
    "target-dir" : "Tellaw/SunshineAdminBundle",

    "scripts": {
      "post-install-cmd": [
        "Fuz\\Symfony\\Collection\\ScriptHandler::postInstall"
      ],
      "post-update-cmd": [
        "Fuz\\Symfony\\Collection\\ScriptHandler::postUpdate"
      ]
    }
}
